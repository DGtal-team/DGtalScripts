// ----------------------- Standard services ------------------------------


///////////////////////////////////////////////////////////////////////////////
// Interface - public :

/**
 * Writes/Displays the object on an output stream.
 * @param out the output stream where the object is written.
 */
template <typename T>
inline
void
YYY::XXX<T>::selfDisplay ( std::ostream & out ) const
{
  out << "[XXX]";
}

/**
 * Checks the validity/consistency of the object.
 * @return 'true' if the object is valid, 'false' otherwise.
 */
template <typename T>
inline
bool
YYY::XXX<T>::isValid() const
{
    return true;
}



///////////////////////////////////////////////////////////////////////////////
// Implementation of inline functions                                        //

template <typename T>
inline
std::ostream&
YYY::operator<< ( std::ostream & out,
                  const XXX<T> & object )
{
  object.selfDisplay( out );
  return out;
}

